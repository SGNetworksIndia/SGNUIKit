		"postcss-url": "^10.1.3",
		"rollup": "^4.5.0",
		"rollup-plugin-license": "^3.0.1",
		"rollup-plugin-postcss": "^4.0.2",
		"rollup-plugin-progress": "^1.1.2",
		"sass-loader": "^13.2.0",
		"style-loader": "^3.3.2",
