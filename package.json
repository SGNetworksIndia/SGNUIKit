		"autoprefixer": "^10.4.14",
		"babel-loader": "^9.1.2",
		"conventional-github-releaser": "^3.1.5",
		"conventional-recommended-bump": "^6.1.0",
		"css-url-relative-plugin": "^1.1.0",
		"del": "^7.0.0",
		"dotenv": "^16.0.3",
		"execa": "^7.1.1",
		"fs-extra": "^11.1.0",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^8.0.0",
		"gulp-clean-css": "^4.3.0",
		"gulp-imagemin": "^8.0.0",
		"gulp-postcss": "^9.0.1",
		"gulp-rename": "^2.0.0",
